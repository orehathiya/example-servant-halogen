{
  "name": "pux-example",
  "version": "1.0.0",
  "description": "pux exapmple interact with servant api",
  "main": "support/index.js",
  "keywords": [
    "pux",
    "purescript-pux"
  ],
  "scripts": {
    "postinstall": "psc-package update",
    "clean": "rimraf static/dist",
    "build": "npm run clean && webpack --config ./webpack.config.js --progress --profile --colors",
    "watch": "npm run clean && webpack-dev-server --content-base static/ --hot --inline --config webpack.config.js",
    "serve": "npm run build && serve -s static",
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/orehathiya/example-servant-persistent.git"
  },
  "author": "orehathiya",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/orehathiya/example-servant-persistent/issues"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "dependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "devDependencies": {
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "pulp": "^12.0.1",
    "purescript": "^0.11.7",
    "purescript-psa": "^0.5.1",
    "purs-loader": "^3.1.0",
    "rimraf": "^2.5.2",
    "serve": "^5.2.4",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.5.4",
    "xhr2": "^0.1.3"
  }
}
